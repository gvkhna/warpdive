  "scripts": {
    "astro": "astro",
    "check": "tsc --noEmit && astro check",
    "db:create": "wrangler d1 execute warpdive-dev-db --local --command 'SELECT NULL'",
    "db:destroy": "rm -rf .wrangler/state/v3/d1",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "CI=true wrangler d1 migrations apply warpdive-dev-db --local",
    "db:seed": "tsx src/db/seed.ts",
    "db:open": "open $(find .wrangler/state/v3/d1/ -type f -name '*.sqlite' -print -quit)",
    "dev": "astro dev --host",
    "maildev": "maildev -v --hide-extensions STARTTLS --mail-directory tmp/maildev/",
    "preview": "astro preview",
    "proto": "tsx scripts/clean-proto-ts.ts && tsx scripts/compile-proto-ts.ts",
    "storybook:build": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test": "jest"
  },
